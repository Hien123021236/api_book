{
  "name": "ima",
  "version": "1.0.0",
  "private": true,
  "description": "IMA Job Management",
  "author": "Geminisoft",
  "apidoc": {
    "title": "API DOCS",
    "version": "1.0.0",
    "url": "http://vdcimarch.zapto.org:7500"
  },
  "engines": {
    "node": ">14.17.0"
  },
  "main": "server.js",
  "scripts": {
    "build": "webpack",
    "eslint": "eslint ./",
    "eslint-fix": "eslint ./ --fix",
    "debug": "nodemon server.js",
    "prod": "node dist/api.bundle.js",
    "pm2Dev": "pm2 start process.json --env development",
    "pm2Pro": "pm2 start process.json --env production",
    "pm2Logs": "pm2 logs ima",
    "pm2Restart": "pm2 restart all",
    "pm2Stop": "pm2 stop all",
    "pm2Delete": "pm2 delete all",
    "genAPIDoc": "apidoc -i app/routes/ -o ./public/docs/",
    "createDocker": "docker build . -t geminisoft/ima_job_manager_nodejs",
    "migrate:create": "npx sequelize-cli migration:create --name",
    "migrate:up": "npx sequelize-cli db:migrate",
    "db:seed:one": "npx sequelize-cli db:seed --seed",
    "db:remove": "npx sequelize-cli db:seed:undo:all --seeders-path ./database/seeders && npx sequelize-cli db:migrate:undo:all",
    "db:init": "npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all --seeders-path ./database/seeders",
    "migrate:undo": "npx sequelize-cli db:migrate:undo"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.3.0",
    "@ladjs/graceful": "^3.2.2",
    "@sendgrid/mail": "^7.7.0",
    "@stomp/stompjs": "^6.1.2",
    "async": "^3.2.0",
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "bree": "^9.1.3",
    "cabin": "^2.0.7",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "cron-parser": "^4.7.0",
    "csv-stringify": "^5.6.2",
    "dotenv": "^16.0.1",
    "elasticsearch": "^16.7.1",
    "express": "^4.17.1",
    "find-remove": "^3.1.0",
    "fs-extra": "^10.0.0",
    "google-auth-library": "^7.10.1",
    "googleapis": "^89.0.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "luxon": "^3.1.1",
    "method-override": "^3.0.0",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.39",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "n-readlines": "^1.0.1",
    "path": "^0.12.7",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "request": "^2.88.0",
    "sequelize": "^6.28.0",
    "socket.io": "^4.5.4",
    "socket.io-redis": "^6.1.0",
    "stomp-client": "^0.9.0",
    "stompit": "^1.0.0",
    "uuid": "^8.3.2",
    "validator": "^13.6.0",
    "worker-thread": "^1.1.0",
    "ws": "^8.11.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "apidoc": "^0.53.1",
    "eslint": "^7.27.0",
    "eslint-config-google": "^0.14.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "sequelize-cli": "^6.4.1",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0"
  },
  "keywords": [
    "nodejs",
    "management",
    "system"
  ],
  "license": "GEMINISOFT"
}
