{
  "name": "axe",
  "description": "Logging add-on to send logs over HTTP to your server in Node and Browser environments. Works with any logger! Chop up your logs consistently! Made for Cabin and Lad.",
  "version": "3.0.3",
  "author": "Nick Baugh <niftylettuce@gmail.com> (http://niftylettuce.com)",
  "ava": {
    "serial": true,
    "failFast": true
  },
  "bugs": {
    "url": "https://github.com/cabinjs/axe/issues"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "contributors": [
    "Nick Baugh <niftylettuce@gmail.com> (http://niftylettuce.com)",
    "Alexis Tyler <xo@wvvw.me> (https://wvvw.me/)"
  ],
  "dependencies": {
    "@ladjs/superagent": "^5.0.0",
    "boolean": "^0.2.0",
    "console-polyfill": "^0.3.0",
    "cuid": "^2.1.4",
    "fast-safe-stringify": "^2.0.6",
    "format-specifiers": "^1.0.0",
    "lodash": "^4.17.11",
    "message-headers": "^1.0.1",
    "parse-err": "^0.0.5",
    "util-format-x": "^1.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@commitlint/cli": "^7.3.2",
    "@commitlint/config-conventional": "^7.3.1",
    "ava": "^1.1.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "caniuse-lite": "^1.0.30000929",
    "codecov": "^3.1.0",
    "consola": "^2.3.2",
    "cross-env": "^5.2.0",
    "eslint": "5.12.0",
    "eslint-config-xo-lass": "^1.0.3",
    "eslint-plugin-compat": "^2.6.3",
    "eslint-plugin-node": "^8.0.1",
    "express": "^4.16.4",
    "fixpack": "^2.3.1",
    "husky": "^1.3.1",
    "jsdom": "13.1.0",
    "koa": "^2.6.2",
    "lint-staged": "^8.1.0",
    "nyc": "^13.1.0",
    "pino": "^5.10.7",
    "remark-cli": "^6.0.1",
    "remark-preset-github": "^0.0.13",
    "rimraf": "^2.6.3",
    "signale": "^1.3.0",
    "sinon": "^7.2.2",
    "tinyify": "^2.5.0",
    "xo": "^0.24.0"
  },
  "engines": {
    "node": ">=8.8.1"
  },
  "files": [
    "lib",
    "dist"
  ],
  "homepage": "https://github.com/cabinjs/axe",
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "jsdelivr": "dist/axe.min.js",
  "keywords": [
    "airbrake",
    "analytic",
    "analytics",
    "api",
    "axe",
    "bugsnag",
    "bunyan",
    "cabin",
    "cabinjs",
    "chalk",
    "color",
    "colored",
    "connect",
    "console",
    "express",
    "hackable",
    "hapi",
    "koa",
    "lad",
    "ladjs",
    "lass",
    "log",
    "logger",
    "logging",
    "loggly",
    "middleware",
    "official",
    "raven",
    "sentry",
    "signale",
    "timber",
    "transport",
    "winston",
    "wrapper"
  ],
  "license": "MIT",
  "lint-staged": {
    "linters": {
      "*.js": [
        "xo --fix",
        "git add"
      ],
      "*.md": [
        "remark . -qfo",
        "git add"
      ],
      "package.json": [
        "fixpack",
        "git add"
      ]
    }
  },
  "main": "lib/index.js",
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": true,
    "trailingComma": "none"
  },
  "remarkConfig": {
    "plugins": [
      "preset-github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cabinjs/axe"
  },
  "scripts": {
    "ava": "cross-env NODE_ENV=test ava",
    "browserify": "browserify src/index.js -o dist/axe.js -s Axe -d -t babelify",
    "build": "npm run build:clean && npm run build:lib && npm run build:dist",
    "build:clean": "rimraf lib dist",
    "build:dist": "npm run browserify && npm run minify",
    "build:lib": "babel src --out-dir lib",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "xo && remark . -qfo && eslint lib",
    "minify": "cross-env NODE_ENV=production browserify src/index.js -o dist/axe.min.js -s Axe -d -t babelify -p tinyify",
    "nyc": "cross-env NODE_ENV=test nyc ava",
    "test": "npm run build && npm run lint && npm run ava",
    "test-coverage": "npm run build && npm run lint && npm run nyc"
  },
  "unpkg": "dist/axe.min.js",
  "xo": {
    "prettier": true,
    "space": true,
    "extends": [
      "xo-lass"
    ],
    "env": [
      "node",
      "browser"
    ],
    "ignore": [
      "config.js"
    ]
  }
}
